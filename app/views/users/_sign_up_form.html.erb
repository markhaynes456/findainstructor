<div class="container sign_up_form">
<%= form_for(@user,html:{"data-toggle":"validator"}) do |f| %>
<div class="form-group has-feedback">
  <%= label_tag("Acount type") %>
  <%= text_field_tag("user[account]",nil,:class=>"form-control has_success",:placeholder=>"Student or Instructor",:max_length=>"60", :required=>true) %>
  <span class="form-control-feedback"></span>
</div>
<div class="form-group hidden">
  <%= f.label(:instructor) %>
  <%= f.check_box(:instructor,:class=>"form-control") %>
</div>
<div class="row">
<div class="form-group col-md-6 has-feedback">
  <%= f.label(:last_name) %>
  <%= f.text_field(:first_name,:class=>"form-control has-feedback",placeholder:"First Name",:max_length=>"60",:required=>true) %>
  <span class="form-control-feedback"></span>
</div>
<div class="form-group col-md-6 has-feedback">
  <%= f.label(:last_name) %>
  <%= f.text_field(:last_name,:class=>"form-control",placeholder:"Last Name",:max_length=>"60",:required=>true) %>
  <span class="form-control-feedback"></span>
</div>
</div>
<div class="form-group has-feedback">
  <%= f.label(:email) %>
  <%= f.email_field(:email,:class=>"form-control has-feedback",placeholder:"jog@blogs.com",:required=>true) %>
  <span class="form-control-feedback"></span>
</div>
<div class="form-group has-feedback">
  <%= f.label(:password) %>
  <%= f.password_field(:password,:class=>"form-control","data-minlength":"6",:required=>true) %>
  <span class="form-control-feedback"></span>
</div>
<div class="form-group has-feedback">
  <%= label_tag("Password check") %>
  <%= password_field_tag("password_check",nil,:class=>"form-control","data-match":"#user_password","data-match-error":"Whoops, these don't match",:required=>true) %>
  <span class="form-control-feedback"></span>
</div>

<div class="help-block with-errors"></div>

  <%= f.submit("Create Account",:class=>"btn btn-primary btn-lg center-block medium-font") %>
</div>

<a class="btn">should fire event</a>
<% end %>
</div>
